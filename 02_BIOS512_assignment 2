   "source": [
    "# uncomment and run to produce the line chart table\n",
    "\n",
    "ncaa_sports_spending2 = ncaa_sports_spending |>\n",
    " group_by(year, men_or_women) |>\n",
    "summarize(expenditure_usd = sum(expenditure_usd, na.rm = TRUE))\n",
    "\n",
    " ncaa_sports_spending2 |> head()"
   ]
